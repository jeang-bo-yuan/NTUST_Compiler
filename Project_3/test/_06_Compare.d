main() {
    int a = 1;
    println a <  10; // 1 1 0 0 0 1
    println a <= 10;
    println a == 10;
    println a >= 10;
    println a >  10;
    println a != 10;

    println "---";

    float b = 1.0f;
    println b <  10.0f;
    println b <= 10.0f;
    println b == 10.0f;
    println b >= 10.0f;
    println b >  10.0f;
    println b != 10.0f;

    println "---";

    double c = 1.0;
    println c <  10.0;
    println c <= 10.0;
    println c == 10.0;
    println c >= 10.0;
    println c >  10.0;
    println c != 10.0;
}